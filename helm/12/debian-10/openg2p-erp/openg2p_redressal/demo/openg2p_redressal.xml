<?xml version="1.0"?>
<odoo noupdate="1">


    <!-- Categories -->

    <record id="helpdesk_mgmt.helpdesk_category_1" model="helpdesk.ticket.category">
        <field name="name">Payment Related</field>
    </record>

    <record id="helpdesk_mgmt.helpdesk_category_2" model="helpdesk.ticket.category">
        <field name="name">Record Update</field>
    </record>

    <record id="helpdesk_mgmt.helpdesk_category_3" model="helpdesk.ticket.category">
        <field name="name">Location Change</field>
    </record>

    <!-- Teams -->

    <record id="helpdesk_mgmt.helpdesk_team_1" model="helpdesk.ticket.team">
        <field name="name">Payments Team</field>
    </record>

    <record id="helpdesk_mgmt.helpdesk_team_2" model="helpdesk.ticket.team">
        <field name="name">Enrollment Team</field>
        <field name="category_ids" eval="[(6,0,[ref('helpdesk_mgmt.helpdesk_category_2')])]"/>
    </record>

    <record id="helpdesk_mgmt.helpdesk_team_3" model="helpdesk.ticket.team">
        <field name="name">Verification Team</field>
        <field name="category_ids" eval="[(6,0,[ref('helpdesk_mgmt.helpdesk_category_2')])]"/>
    </record>

    <!-- Tags -->

    <record id="helpdesk_mgmt.helpdesk_tag_1" model="helpdesk.ticket.tag">
        <field name="name"></field>
    </record>

    <record id="helpdesk_mgmt.helpdesk_tag_2" model="helpdesk.ticket.tag">
        <field name="name"></field>
    </record>

    <record id="helpdesk_mgmt.helpdesk_tag_3" model="helpdesk.ticket.tag">
        <field name="name"></field>
    </record>

    <!-- Tickets -->

    <record id="helpdesk_mgmt.helpdesk_ticket_1" model="helpdesk.ticket">
        <field name="name">Wrong Payment Amount Received</field>
        <field name="description"></field>
        <field name="program_id" ref="openg2p_program.program_0"/>
        <field name="beneficiary_id" ref="openg2p.openg2p_beneficiary_1"/>
    </record>

    <record id="helpdesk_mgmt.helpdesk_ticket_2" model="helpdesk.ticket">
        <field name="name">Payment was not received</field>
        <field name="description"></field>
        <field name="program_id" ref="openg2p_program.program_0"/>
        <field name="beneficiary_id" ref="openg2p.openg2p_beneficiary_2"/>
    </record>

    <record id="helpdesk_mgmt.helpdesk_ticket_3" model="helpdesk.ticket">
        <field name="name">Location Listed Wrongly</field>
        <field name="description"></field>
        <field name="program_id" ref="openg2p_program.program_0"/>
        <field name="beneficiary_id" ref="openg2p.openg2p_beneficiary_3"/>
    </record>

    <record id="helpdesk_mgmt.helpdesk_ticket_4" model="helpdesk.ticket">
        <field name="name">Wish to change emergency contact</field>
        <field name="description"></field>
        <field name="program_id" ref="openg2p_program.program_0"/>
        <field name="beneficiary_id" ref="openg2p.openg2p_beneficiary_4"/>
    </record>

    <record id="helpdesk_mgmt.helpdesk_ticket_5" model="helpdesk.ticket">
        <field name="name">No payment received for March disbursement</field>
        <field name="description"></field>
        <field name="program_id" ref="openg2p_program.program_0"/>
        <field name="beneficiary_id" ref="openg2p.openg2p_beneficiary_1"/>
    </record>

    <record id="helpdesk_mgmt.helpdesk_ticket_6" model="helpdesk.ticket">
        <field name="name">Wishes to change duty station</field>
        <field name="description"></field>
        <field name="program_id" ref="openg2p_program.program_0"/>
        <field name="beneficiary_id" ref="openg2p.openg2p_beneficiary_1"/>
    </record>

    <record id="helpdesk_mgmt.helpdesk_ticket_7" model="helpdesk.ticket">
        <field name="name">Complaint related to late payment</field>
        <field name="description"></field>
        <field name="program_id" ref="openg2p_program.program_0"/>
        <field name="beneficiary_id" ref="openg2p.openg2p_beneficiary_2"/>
    </record>

</odoo>
