<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="digest_digest_view_form" model="ir.ui.view">
        <field name="name">digest.digest.view.form.inherit.openg2p.registration</field>
        <field name="model">digest.digest</field>
        <field name="inherit_id" ref="digest.digest_digest_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='kpi_general']" position="after">
                <group name="kpi_openg2p" string="Registration" groups="openg2p_registration.group_openg2p_registration_user">
                    <field name="kpi_openg2p_registration_new_beneficiaries"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
