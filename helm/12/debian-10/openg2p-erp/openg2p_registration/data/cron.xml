<?xml version="1.0"?>
<odoo>
    <data>

        <record id="ir_cron_openg2p_registration_duplicate_check" model="ir.cron">
            <field name="name">Check Registrations for Duplicate</field>
            <field name="model_id" ref="model_openg2p_registration"/>
            <field name="state">code</field>
            <field name="code">model.cron_check_uniqueness()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>

    </data>

</odoo>